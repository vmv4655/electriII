#: import hex kivy.utils.get_color_from_hex
#: import mainFile main
#: import sla kivy.adapters.simplelistadapter
#: import label kivy.uix.label
#: import ListItemButton kivy.uix.listview.ListItemButton
#: import ListAdapter kivy.adapters.listadapter.ListAdapter
<CustomButton@Button>:
	spacing: 10
	font_size: 32
	size_hint: None, None
	size: 130, 50

<DataLabel@Label>
	text_size: self.size
	font_size: 20
	halign:    'left'
	valign:    'middle'
	color: 0, 0, 0, 1

<SecondRowDataLabel@Label>
	text_size: self.size
	font_size: 15
	halign:    'left'
	valign:    'middle'
	color: 0, 0, 0, 1

<AppLayout>:
	displaylistaCargas: listaCargas
	id           : appLayout
	tipoDeFuente : tipoFuente
	displayVan   : _Van
	displayVbn   : _Vbn 
	displayVcn   : _Vcn
	displayVab   : _Vab
	displayVbc   : _Vbc
	displayVca   : _Vca
	displayIa    : Ia
	displayIb    : Ib
	displayIc    : Ic
	displayIn    : In
	displayFP    : FP
	displayL3W   : L3W
	displayL2W   : L2W

	
	BoxLayout:
		size: self.parent.size
		pos : self.parent.pos
		orientation: "vertical"


		#Seccion superior
		BoxLayout:
			size_hint_y: .20
			orientation: "vertical"
			spacing: 10
			BoxLayout:
				orientation: "horizontal"
				canvas:
					Color:
						rgb: hex('#D0D3D4')
					Rectangle:
        				size: self.size
        				pos: self.pos
				BoxLayout:
					orientation: "vertical"
					size_hint_x: .45
					Button:
						background_color: hex('#cd035a')
						text: "Seleccionar voltaje"
						on_press: app.appLayout.getVoltajeAB()

					BoxLayout:
						size_hint_y: .7
						orientation: "vertical"
						Label:
							bold:  True
							color: 0, 0, 0, 1
							text: "Fuente selecionada: "


						Label:
							id: tipoFuente
							#bold:      True
							color: 0, 0, 0, 1
							text: ""

					Button:
						background_color: hex('#cd035a')
						text: "Seleccionar fuente"
						on_press: root.getTipoFuente()
				
				BoxLayout:
					orientation: "vertical"
					BoxLayout:
						orientation: "horizontal"
						DataOutLabel:
							text: "Van: "
						DataLabel	
							id: _Van
							text: ""

					BoxLayout:
						orientation: "horizontal"
						DataOutLabel:
							text: "Vbn: "
						DataLabel	
							id: _Vbn
							text: ""

					BoxLayout:
						orientation: "horizontal"
						DataOutLabel:
							text: "Vcn: "
						DataLabel	
							id: _Vcn
							text: ""

					BoxLayout:
						orientation: "horizontal"
						DataOutLabel:
							text: "Vab: "
						DataLabel	
							id: _Vab
							text: ""
					
					BoxLayout:
						orientation: "horizontal"
						DataOutLabel:
							text: "Vbc: "
						DataLabel	
							id: _Vbc
							text: ""

					BoxLayout:
						orientation: "horizontal"
						DataOutLabel:
							text: "Vca: "
						DataLabel:	
							id: _Vca
							text: ""

				BoxLayout:
					orientation: "vertical"
					BoxLayout:
						orientation: "horizontal"
						SecondRowDataOutLabel:
							text: "Ia: "
						SecondRowDataLabel:
							id: Ia

					BoxLayout:
						orientation: "horizontal"
						SecondRowDataOutLabel:
							text: "Ib: "
						SecondRowDataLabel:
							id: Ib

					BoxLayout:
						orientation: "horizontal"
						SecondRowDataOutLabel:
							text: "Ic: "
						SecondRowDataLabel:
							id: Ic

					BoxLayout:
						orientation: "horizontal"
						SecondRowDataOutLabel:
							text: "In: "
						SecondRowDataLabel:
							id: In

					BoxLayout:
						orientation: "horizontal"
						SecondRowDataOutLabel:
							text: "FP: "
						SecondRowDataLabel:
							id: FP

					BoxLayout:
						orientation: "horizontal"
						SecondRowDataOutLabel:
							text: "L3W: "
						SecondRowDataLabel:
							id: L3W

					BoxLayout:
						orientation: "horizontal"
						SecondRowDataOutLabel:
							text: "L2W: "
						SecondRowDataLabel:
							id: L2W

		#Seccion inferior
		BoxLayout:
			orientation: "vertical"
			size_hint_y: .65

			BoxLayout:
				size_hint_x: 1
				size_hint_y: .08
				orientation: "horizontal"
				canvas:
					Color:
						rgba: hex('#212F3C')
					Rectangle:
        				size: self.size
        				pos: self.pos

				Label:
					size_hint_x: .05
					font_size: 24
					text: "Cargas"

				BoxLayout:
					size_hint_x: .02

					Button:
						background_color: hex('#cd035a')
						size_hint_x: .2
						text: "Agregar Carga"
						font_size: 23
						on_press: app.appLayout.getTipoCarga()
			
			
			BoxLayout:
				canvas:
					Color:
						rgb: hex("#D0D3D4")
					Rectangle:
        				size: self.size
        				pos: self.pos

        		ListView:
        			id: listaCargas
        			adapter:
        				ListAdapter(data=[], cls= mainFile.BotonListaCargas)

        		#ListView:
        			#adapter:
        				#sla.SimpleListAdapter(data=["Motor1", "Motor2", "Motor3"], cls=label.Label)
        		


<SelecVab>
	#auto_dismiss: False
	size_hint: None, None
	size: 400, 250
	title: "Digite el voltaje AB"
	separator_color: hex('#740233')

	BoxLayout:
		size: self.parent.size
		pos : self.parent.pos
		orientation: "vertical"

		BoxLayout:
			padding: 10
			#size_hint_x: .75
			orientation: "horizontal"
			
			ToggleButton:
				id: ABC
				background_color: hex('#cd035a')
				text: "ABC"
				group: "secuencia"
				state: "down"	

			ToggleButton:
				id: ACB
				background_color: hex('#cd035a')
				text: "ACB"
				group: "secuencia"
				state: "normal"

		BoxLayout:
			orientation: "horizontal"
			size_hint_x: .3
			CheckBox:
				id: PolSelected
				color: 1, 1, 1, 1 #hex('#2ECC71')
				group: "recOrPol"
				size_hint_x: .05
				active: True

			Label:
				text: "Forma Polar"
				size_hint_x: .15


		BoxLayout:
			size_hint_x: .7
			orientation: "horizontal"

			BoxLayout:
				size_hint_x: .25
			
			TextInput:
				id: vMagnitud
				hint_text: "Magnitud"
				input_filter: "float"
				write_tab: False
				disabled: not PolSelected.active
			TextInput:
				id: vAngulo
				hint_text: "Angulo"
				input_filter: "float"
				write_tab: False
				disabled: not PolSelected.active

		BoxLayout:
			orientation: "horizontal"
			size_hint_x: .4

			CheckBox:
				id: RectSelected
				color: 1, 1, 1, 1 #hex('#2ECC71')
				group: "recOrPol"
				size_hint_x: .04

			Label:
				text: "Forma Rectangular"
				size_hint_x: .15

		BoxLayout:
			size_hint_x: .7
			orientation: "horizontal"
			
			BoxLayout:
				size_hint_x: .25

			TextInput:
				id: vReal
				hint_text: "Real"
				input_filter: "float"
				write_tab: False
				disabled: not RectSelected.active
			
			TextInput:
				id: vComplejo
				hint_text: "Complejo (j)"
				input_filter: "float"
				write_tab: False
				disabled: not RectSelected.active
		BoxLayout:
			size_hint_y: .3

		BoxLayout:
			size_hint_y: 1
			orientation: "horizontal"

			Button:
				background_color: hex('#cd035a')
				text: "Regresar"
				on_press:
					root.dismiss()
					app.appLayout.getTipoFuente()

			Button:
				background_color: hex('#cd035a')
				text: "Aceptar"
				on_press:
					root.dismiss()
					app.appLayout.setVoltajeAB(ABC.state, PolSelected.state, vMagnitud.text, vAngulo.text, vReal.text, vComplejo.text)

		


<DataOutLabel@Label>
	text_size: self.size
	size_hint_x: .2
	font_size: 20	
	halign:    'left'
	valign:    'middle'
	bold:      True
	color: 0, 0, 0, 1

<SecondRowDataOutLabel@Label>
	text_size: self.size
	size_hint_x: .2
	font_size: 15	
	halign:    'left'
	valign:    'middle'
	bold:      True
	color: 0, 0, 0, 1



<SelecFuente>
	#auto_dismiss: False
	size_hint: None, None
	size: 400, 200
	title: "Seleccione el tipo de fuente"
	separator_color: hex('#740233')
	BoxLayout
		spacing: 2
		size: self.parent.size
		pos : self.parent.pos
		orientation: "horizontal"
	
		Button:
			pading: 10
			background_color: hex('#cd035a')
			text: "Estrella"
			font_size: 32
			size: self.parent.size
			pos: self.parent.pos
			on_release: 
				root.dismiss()
				app.appLayout.setTipoFuente('Estrella')
		
		Button:
			pading: 300
			background_color: hex('#cd035a')
			text: "Delta"
			font_size: 32
			size: self.parent.size
			on_release: 
				root.dismiss()
				app.appLayout.setTipoFuente('Delta')

<textoTiposDeFuente@Label>
	text_size: self.size
	size_hint_x: .2
	font_size: 20	
	halign:    'left'
	valign:    'middle'
	#bold:      True

<selectCarga>
	size_hint: None, None
	size: 400, 200
	title: "Seleccione el tipo de carga"
	separator_color: hex('#740233')
	BoxLayout:
		orientation: "vertical"
		size: self.parent.size
		pos : self.parent.pos
		BoxLayout:
			size_hint_x: .5
			orientation: "horizontal"
			CheckBox:
				id: trifasicaSelected
				color: 1, 1, 1, 1 #hex('#2ECC71')
				group: "tipoCarga"
				size_hint_x: .05
				active: True

			textoTiposDeFuente:
				text: "Trifasica"
				size_hint_x: .15

		BoxLayout:
			size_hint_x: .5
			orientation: "horizontal"
			CheckBox:
				id: monofaseSelected
				text: "Monofase"
				color: 1, 1, 1, 1 #hex('#2ECC71')
				group: "tipoCarga"
				size_hint_x: .05
				active: False

			textoTiposDeFuente:
				text: "Monofasica"
				size_hint_x: .15

		BoxLayout:
			size_hint_x: .5
			orientation: "horizontal"
			CheckBox:
				id: motorSelected
				text: "Motor"
				color: 1, 1, 1, 1 #hex('#2ECC71')
				group: "tipoCarga"
				size_hint_x: .05
				active: False

			textoTiposDeFuente:
				text: "Motor"
				size_hint_x: .15

		BoxLayout:
			size_hint_y: 1
			orientation: "horizontal"

			Button:
				background_color: hex('#cd035a')
				id: buttonCancelarTipoCarga
				text: "Cancelar"
				on_press:
					root.dismiss()

			Button:
				background_color: hex('#cd035a')
				id: buttonAceptarTipoCarga
				text: "Aceptar"
				on_press:
					root.dismiss()
					app.appLayout.setTipoCarga(trifasicaSelected.active, monofaseSelected.active, motorSelected.active)
					
<GetCargaMonofase>
	size_hint: None, None
	size: 400, 380
	title: "Agregar carga monofasica"
	separator_color: hex('#740233')
	BoxLayout:
		orientation: "vertical"
		size: self.parent.size
		pos : self.parent.pos

		BoxLayout:
			#size_hint_y: .1
			size_hint_x: .5

			Label:
				text: "Cantidad de cargas:"

			TextInput:
				size_hint_x: .2
				id: cantidadCargas
				text: "1"
				input_filter: "float"
				write_tab: False

		BoxLayout:
			#size_hint_x: .9
			#size_hint_y: .1
			orientation: "horizontal"
			
			Label:
				text: "Seleccione en cuales fases esta conectada esta carga"
				#size_hint_x: 1

		BoxLayout:
			#size_hint_y: .1
			orientation: "horizontal"
			size: self.parent.size
			pos : self.parent.pos

			BoxLayout:
				#size_hint_x: .5
				orientation: "horizontal"
				CheckBox:
					id: a1Selected
					color: 1, 1, 1, 1 #0, 1, 0, 1 #hex('#2ECC71')
					group: "fase1"
					#size_hint_x: .05
					active: True

				textoTiposDeFuente:
					text: "A"
					#size_hint_x: .15
			BoxLayout:
				#size_hint_x: .5
				orientation: "horizontal"
				CheckBox:
					id: b1Selected
					color: 1, 1, 1, 1 #0, 1, 0, 1 #hex('#2ECC71')
					group: "fase1"
					#size_hint_x: .05
					active: False

				textoTiposDeFuente:
					text: "B"
					#size_hint_x: .15

			BoxLayout:
				#size_hint_x: .5
				orientation: "horizontal"
				CheckBox:
					id: c1Selected
					color: 1, 1, 1, 1 #hex('#2ECC71')
					group: "fase1"
					#size_hint_x: .05
					active: False

				textoTiposDeFuente:
					text: "C"
					#size_hint_x: .15

			BoxLayout:
				#size_hint_x: .5
				orientation: "horizontal"
				CheckBox:
					id: n1Selected
					color: 1, 1, 1, 1 #hex('#2ECC71')
					group: "fase1"
					#size_hint_x: .05
					active: False

				textoTiposDeFuente:
					text: "N"
					#size_hint_x: .15

		BoxLayout:
			#size_hint_y: .1
			orientation: "horizontal"
			size: self.parent.size
			pos : self.parent.pos
			
			BoxLayout:
				#size_hint_x: .5
				orientation: "horizontal"
				CheckBox:
					id: a2Selected
					color: 1, 1, 1, 1 #hex('#2ECC71')
					group: "fase2"
					#size_hint_x: .05
					active: False

				textoTiposDeFuente:
					text: "A"
					#size_hint_x: .15
			BoxLayout:
				#size_hint_x: .5
				orientation: "horizontal"
				CheckBox:
					id: b2Selected
					color: 1, 1, 1, 1 #hex('#2ECC71')
					group: "fase2"
					#size_hint_x: .05
					active: True

				textoTiposDeFuente:
					text: "B"
					#size_hint_x: .15

			BoxLayout:
				#size_hint_x: .5
				orientation: "horizontal"
				CheckBox:
					id: c2Selected
					color: 1, 1, 1, 1 #hex('#2ECC71')
					group: "fase2"
					#size_hint_x: .05
					active: False

				textoTiposDeFuente:
					text: "C"
					#size_hint_x: .15

			BoxLayout:
				#size_hint_x: .5
				orientation: "horizontal"
				CheckBox:
					id: n2Selected
					color: 1, 1, 1, 1 #hex('#2ECC71')
					group: "fase2"
					#size_hint_x: .05
					active: False

				textoTiposDeFuente:
					text: "N"
					#size_hint_x: .15

		BoxLayout:
			orientation: "horizontal"
			#size_hint_y: .2
			size_hint_x: .6
			CheckBox:
				id: potenciaSelected
				color: 1, 1, 1, 1 #hex('#2ECC71')
				group: "potOrZ"
				size_hint_x: .1
				active: True

			Label:
				text: "Potencia y factor de potencia"

		BoxLayout:
			#size_hint_x: 1
			padding: 2
			orientation: "horizontal"
			
			TextInput:
				id: aparenteSelected
				hint_text: "Aparente (VA)"
				input_filter: "float"
				write_tab: False
				disabled: (not potenciaSelected.active) or (reactivaSelected.text != "" or realSelected.text != "")

			TextInput:
				id: reactivaSelected
				hint_text: "Reactiva (VAr)"
				input_filter: "float"
				write_tab: False
				disabled: (not potenciaSelected.active) or (aparenteSelected.text != "" or realSelected.text != "")

			TextInput:
				id: realSelected
				hint_text: "Real (W)"
				input_filter: "float"
				write_tab: False
				disabled: (not potenciaSelected.active) or (aparenteSelected.text != "" or reactivaSelected.text != "")
			
		BoxLayout:
			#size_hint_x: 1
			padding: 2
			orientation: "horizontal"
			BoxLayout:
				size_hint_x: .2
			TextInput:
				size_hint_x: .4
				id: fpSelected
				hint_text: "FP (0-1)"
				input_filter: "float"
				write_tab: False
				disabled: not potenciaSelected.active
			BoxLayout:
				size_hint_x: .1
			BoxLayout:
				orientation: "horizontal"
				size_hint_x: .45
				CheckBox:
					id: adelantoSelected
					
					color: 1, 1, 1, 1 #hex('#2ECC71')
					group: "atrazoOrAdelanto"
					size_hint_x: .2
					active: True

				Label:
					text: "Adelanto"

			BoxLayout:
				orientation: "horizontal"
				size_hint_x: .4
				CheckBox:
					id: atrasoSelected
					color: 1, 1, 1, 1 #hex('#2ECC71')
					group: "atrazoOrAdelanto"
					size_hint_x: .1
					active: False

				Label:
					text: "Atraso"

		BoxLayout:
			orientation: "horizontal"
			size_hint_x: .28
			CheckBox:
				id: zSelected
				color: 1, 1, 1, 1 #hex('#2ECC71')
				group: "potOrZ"
				size_hint_x: .3
				active: False

			Label:
				text: "Impedancia"
				#size_hint_x: .15

		BoxLayout:
			#size_hint_x: 1
			#size_hint_y: .1
			#padding: 2
			orientation: "horizontal"
			BoxLayout:
				size_hint_x: .3
			TextInput:
				id: realzSelected
				hint_text: "Real"
				input_filter: "float"
				write_tab: False
				disabled: not zSelected.active

			TextInput:
				id: complejoSelected
				hint_text: "Complejo (j)"
				input_filter: "float"
				write_tab: False
				disabled: not zSelected.active

			BoxLayout:
				size_hint_x: .3


		BoxLayout:
			#size_hint_y: .1
			orientation: "horizontal"

			Button:
				background_color: hex('#cd035a') #0, 1, 0, 1 #hex('#2ECC71')
				text: "Cancelar"
				on_press:
					root.dismiss()

			Button:
				background_color: hex('#cd035a') #0, 1, 0, 1 #hex('#2ECC71')
				text: "Aceptar"
				on_press:
					root.dismiss()
					app.appLayout.addCargaMonoFase(cantidadCargas.text, a1Selected.active, a2Selected.active, b1Selected.active, b2Selected.active, c1Selected.active, c2Selected.active,
					n1Selected.active, n2Selected.active,
					potenciaSelected.active, zSelected.active,
					aparenteSelected.text, reactivaSelected.text, realSelected.text, fpSelected.text, adelantoSelected.active, atrasoSelected.active, realzSelected.text, complejoSelected.text)	

<GetCargaMotorMonofase>
